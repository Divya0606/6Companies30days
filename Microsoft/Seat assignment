class Solution {
public:
    double nthPersonGetsNthSeat(int n) {
        float max;
        float a=1;
        if(n==1){
            max=1;
            
        }
        else {
           max= a/2;
        
        }
        // else{
        //     max=(a/(n-1));
        // }
        return max;
    }
};
